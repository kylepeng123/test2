<html>
<title>DemoHome</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<!-- styles -->
<link href="http://netdna.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link href="stylesheets/style.css" rel="stylesheet" media="screen">
<link rel="icon" href="http://i.istockimg.com/file_thumbview_approve/79813167/3/stock-illustration-79813167-polar-bear-standing-icon-design.jpg">

<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="http://code.jquery.com/jquery-1.11.2.min.js" type="text/javascript"></script>
<script src="http://netdna.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js" type="text/javascript"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.12/angular.min.js"></script>
<head>
<script>



    $(document).ready(function() {

        $( "#login" ).click(function() {
            window.location="http://localhost:3000/demoS";
        });


    });
    $(function() {
        var $user_list = $("#user_list");
    $.ajax({
        type: "GET",
        url: "http://localhost:3000/get_user_list",
        data: {},
        success: function(result) {
            console.log(result);
            $user_list.empty();
           // var string;
            $.each(result, function(index, value) {
                console.log(result[index].first_name);
                $user_list.append("<li><a value="+result[index].user_id+">" + result[index].first_name+"  "+result[index].last_name + "</a></li>");
            });
        }
    });
    });

</script>
</head>
<body>
<h1>Demo Home Page</h1>

<form action="/Myform" method="post">

    <input type="button" id="login" value="go to another page" onClick="click()"/>

</form>
<h1>The below is testing database!</h1>
<ul id ="user_list">
</ul>
</body>
</html>
<html>
<title>DemoHome</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<!-- styles -->
<link href="http://netdna.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link href="stylesheets/style.css" rel="stylesheet" media="screen">
<link rel="icon" href="http://i.istockimg.com/file_thumbview_approve/79813167/3/stock-illustration-79813167-polar-bear-standing-icon-design.jpg">

<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="http://code.jquery.com/jquery-1.11.2.min.js" type="text/javascript"></script>
<script src="http://netdna.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js" type="text/javascript"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.12/angular.min.js"></script>
<head>
	<script type="text/javascript">
		function start(){
			var button = document.getElementById( "c1" );
			button.addEventListener( "click", start1, false );

			var button = document.getElementById( "c2" );
			button.addEventListener( "click", start2, false );

		}

		function start1(){

			var input1=document.getElementById( "t1" ).value;



			document.getElementById("o1").innerHTML = input1;}


		function start2(){

			var input2=document.getElementById( "t2" ).value;



			document.getElementById("o2").innerHTML = input2;}

		window.addEventListener( "load", start, false );
	</script>
</head>

<body>
<form>
    <h1>Demo seperate page</h1>
    <p><input id="t1" type="text" name="mytext1" required />
    <button id="c1"type="button">Click1</button>
	</p>
	<p id="o1">
	</p>
	<p>
	<input id="t2" type="text" name="mytext2" required />
     <button id="c2"type="button">Click2</button>
	</p>
	<p id="o2">
	</p>
	
</form>
</body>

</html>